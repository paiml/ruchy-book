fun is_even(number) {
    return number % 2 == 0
}

fun grade_letter(score) {
    if score >= 90 {
        return "A"
    } else if score >= 80 {
        return "B"
    } else if score >= 70 {
        return "C"
    } else if score >= 60 {
        return "D"
    } else {
        return "F"
    }
}

fun describe_number(n) {
    let even_odd = if is_even(n) { "even" } else { "odd" }
    let size = if n > 100 { "large" } else { "small" }
    return f"{n} is a {size} {even_odd} number"
}

// Test all branches for 100% coverage
println(describe_number(42))   // small even
println(describe_number(150))  // large even  
println(describe_number(43))   // small odd
println(describe_number(101))  // large odd

// Test all grade branches
println(grade_letter(95))      // A
println(grade_letter(85))      // B
println(grade_letter(75))      // C
println(grade_letter(65))      // D
println(grade_letter(55))      // F