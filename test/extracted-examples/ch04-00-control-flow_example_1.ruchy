// Ch04: Control Flow - If/Else Grading
let score = 85
let grade = if score >= 90 {
    "A"
} else if score >= 80 {
    "B"
} else {
    "C"
}

// Test all branches for 100% coverage
println(f"Score {score} = Grade {grade}")

// Test A grade
let high_score = 95
let high_grade = if high_score >= 90 { "A" } else if high_score >= 80 { "B" } else { "C" }
println(f"Score {high_score} = Grade {high_grade}")

// Test C grade
let low_score = 65  
let low_grade = if low_score >= 90 { "A" } else if low_score >= 80 { "B" } else { "C" }
println(f"Score {low_score} = Grade {low_grade}")