// Ch13: Performance - Algorithm Comparison
let data = [1, 2, 3, 4, 5]

// Approach 1: Direct calculation
let direct_sum = 1 + 2 + 3 + 4 + 5
println(f"Direct sum: {direct_sum}")

// Approach 2: Formula calculation  
let n = 5
let formula_sum = n * (n + 1) / 2
println(f"Formula sum: {formula_sum}")

// Performance comparison
println(f"Results match: {direct_sum == formula_sum}")
println("Formula is O(1), direct is O(n)")

// Demonstrate with larger number
let large_n = 100
let large_formula = large_n * (large_n + 1) / 2
println(f"Sum of 1 to {large_n}: {large_formula}")