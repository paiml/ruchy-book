// ❌ Error
let mut s = String::from("hello")
let r1 = &mut s
let r2 = &mut s  // Error: cannot borrow as mutable more than once
println(r1)
println(r2)

// ✅ Fixed - Use references sequentially
let mut s = String::from("hello")
{
    let r1 = &mut s
    // r1 scope ends here
}
let r2 = &mut s  // Now ok