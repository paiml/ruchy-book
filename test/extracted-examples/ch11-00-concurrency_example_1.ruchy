// Ch11: Concurrency - Basic Async Concepts
fun process_data(id) {
    println(f"Processing item {id}")
    return id * 2
}

// Simulate concurrent processing
let items = [1, 2, 3, 4, 5]

// Process each item and display results
for item in items {
    let result = process_data(item)
    println(f"Result: {result}")
}

println("Processing complete!")