// Single bound
fn print_it<T: Display>(item: T) {
    println("{}", item)
}

// Multiple bounds
fn compare_and_print<T: Display + PartialOrd>(a: T, b: T) {
    if a > b {
        println("a is greater")
    } else {
        println("b is greater or equal")
    }
}

