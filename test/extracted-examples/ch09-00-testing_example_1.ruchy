// Ch09: Testing - Test Functions
fun add(a, b) {
    return a + b
}

fun is_positive(n) {
    return n > 0
}

// Test cases with expected results
let test1_result = add(2, 3)
let test1_expected = 5
println(f"add(2, 3) = {test1_result}, expected: {test1_expected}")

let test2_result = is_positive(10)
let test2_expected = true
println(f"is_positive(10) = {test2_result}, expected: {test2_expected}")

let test3_result = is_positive(-5)
let test3_expected = false
println(f"is_positive(-5) = {test3_result}, expected: {test3_expected}")